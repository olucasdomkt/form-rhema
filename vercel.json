{
  "functions": {
    "api/lead.js": {
      "runtime": "@vercel/node"
    },
    "api/auth/status.js": {
      "runtime": "@vercel/node"
    },
    "api/auth/authorize.js": {
      "runtime": "@vercel/node"
    },
    "api/auth/callback.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 